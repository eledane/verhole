<html>
	<head>
		<title>{$subject}</title>
	</head>
	<body>
		<h1 style="font-size: 14px;">{$subject}</h1>

		<table>
		{foreach $data as $key => $val}
			<tr>
				<td><strong>{$key}</strong><td>
				<td>{$val|noescape}<td>
			</tr>
		{/foreach}
		</table>

		<br /><br />

		<small>Odeslano: {$date|date: '%d.%m.%Y'}</small>
		</table>
	</body>
</html>
