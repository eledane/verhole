ul,li{
    margin:0;
    padding:0
}
li {
        list-style: none outside none;
    }

    
.clearfix:after { content: "."; display: block; height: 0; clear: both;}
.clearfix { display: inline-block; }
* html .clearfix { height: 1%; }
.clearfix { display: block; }

.server-kv{
    position:relative;
    width:100%;
}

.server-kv img{
    width:100%;
    height:auto;
}


.optionservice{
    position:relative;
