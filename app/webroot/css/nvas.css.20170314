.server-nav{
     width:52%;
     margin:0 auto;
     height:62px;
}

.server-nav ul{
    width:100%;
}

.server-nav ul li{
    float:left;
    width:50%;
    font-size:26px;
    color:#000;
    background:#adadad;
    height:62px;
    line-height:62px;
    text-align:center;
    cursor:pointer;
    margin:0;
}

.server-nav ul li.leftli{
    border-top-left-radius:10px;
}

.server-nav ul li.rightli{
    border-top-right-radius:10px;
}


.server-nav ul li.leftli.active{
    -webkit-box-shadow:0 0 20px #d3d3d3;  
    -moz-box-shadow:0 0 20px #d3d3d3;  
    box-shadow:0 0 20px #d3d3d3; 
    background:#fff; 
    z-index:9
}


.server-nav ul li.rightli.active{
    -webkit-box-shadow:0 0 20px #d3d3d3;  
    -moz-box-shadow:0 0 20px #d3d3d3;  
    box-shadow:0 0 20px #d3d3d3; 
    background:#fff; 
    z-index:9
}



